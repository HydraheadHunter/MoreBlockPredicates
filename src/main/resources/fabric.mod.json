{
  "schemaVersion": 1,
  "id": "vbm",
  "version": "${version}",

  "name": "Variable Block Models",
  "description": "This mod allows users to change block textures dependant on what biome they're placed in.",
  "authors": [
    "omoflop",
    "fennekal"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Sconed/FabricBiomeTextures"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.omoflop.vbm.VariableBlockModels"
    ],
    "client":[
      "mod.omoflop.vbm.VariableBlockModelsClient"
    ]
  },
  "mixins": [
    "vbm.mixins.json"
  ],
  "accessWidener": "vbm.accesswidener",
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
